{
  "name": "llp-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "realstart": "nuxt start",
    "start": "npm run generate && npm run realstart",
    "generate": "nuxt generate",
